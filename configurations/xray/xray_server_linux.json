{
  "log": {
    "loglevel": "none", //  "debug" | "info" | "warning" | "error" | "none"
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log"
  },

  "inbounds": [
//VLESS_XTLS_Fallback
    {
      "port": 443,
      //"listen": "127.0.0.1",
      "protocol": "vless",
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "settings": {
        "clients": [
          {
            "id": "${uuid}", // 填写你的 UUID
            "flow": "xtls-rprx-direct",
            "level": 0,
            "email": "love@v2fly.org"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            //"dest": "/dev/shm/default.sock",
            "dest": ${fallback_port_http},
            "xver": 1
          },
          {
            "alpn": "h2",
            "dest": ${fallback_port_http2},
            //"dest": "/dev/shm/h2c.sock",
            "xver": 1
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "xtls",
        "xtlsSettings": {
          "alpn": ["http/1.1","h2"],
          "minVersion": "1.2",
          "certificates": [
            {
              "ocspStapling": 0,
              "certificateFile": "/etc/ssl/web.crt", // 换成你的证书，绝对路径
              "keyFile": "/etc/ssl/web.key" // 换成你的私钥，绝对路径
            }
          ]
        }
      }
    }
  ],

  "outbounds": [
    {
      "protocol": "freedom",
      "sendThrough": "0.0.0.0",
      "settings": {
        "domainStrategy": "UseIPv4"
      }
    },
    //{
    //  "tag":"IPv6_out",
    //  "protocol": "freedom",
    //  "sendThrough": "::",
    //  "settings": {
    //    "domainStrategy": "UseIPv6" // 指定使用 IPv6
    //  }
    //},
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "block"
    }
  ],
  
  "dns": {
    "hosts": {
      "dns.google": "8.8.4.4",
      "dns.google": "2001:4860:4860::8844"
    },
    "servers": [
    "https://dns.google/dns-query"
    ]
  },

  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      //{
      //  "type": "field",
      //  "outboundTag": "IPv6_out",
      //  "domain": [
      //    "geosite:netflix",
      //    "nflxvideo.net",
      //    "nflxext.com",
      //    "nflxso.net",
      //    "scholar.google.cn",
      //    "scholar.google.com",
      //    "scholar.google.com.hk",
      //    "scholar.google.com.tw",
      //    "scholar.google.com.sg"
      //  ]
      //},
      {
        "type": "field",
        "outboundTag": "block",
        "protocol": [
          "bittorrent"
        ]
      }
    ]
  }
}
